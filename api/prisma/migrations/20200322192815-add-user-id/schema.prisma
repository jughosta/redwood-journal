datasource DS {
  provider = "sqlite"
  url = "***"
}

generator photonjs {
  provider = "prisma-client-js"
  binaryTargets = ["native", env("BINARY_TARGET")]
}

enum DayTime {
  MORNING
  EVENING
}

model Entry {
  id          String @id  @default(cuid())
  question    String
  answer      String
  dayTime     DayTime
  day         String
  userId      String

  @@index([day, userId])
}
